<script>
var g=6;
$("#bt1").click(function () {
	$("#t2").show();
	$("#t2").animate({left:"50px"},2000);
	$("#t2").css("transform","rotate(360deg)");
	$("span").show(1000);
	$("hr").show();
	$("input").val("0");
});

$("#bt2").click(function () {
$("#t2").animate({left:"60px"},2000);
//$("#t2").css("transform","rotate(360deg)");
$("span").hide(2000);
	$("#t2").hide(2000);
});

$("#bt4").click(function () {
var r1=Math.random();
var r2=	Math.floor(r1*6+1);
var r3=$("#e1").val();
var w=parseFloat(r3)+parseFloat(r2);

$("#e1").val(" ");
$("#e1").val(w);
$('#a'+w).css("background-color","black");
});
$("#bt3").click(function () {
	$("#t2").slideToggle("slow");
	$("span").slideToggle("slow");
});
	
</script>

